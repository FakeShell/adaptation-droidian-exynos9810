#!/usr/bin/make -f

%:
	dh $@

override_dh_auto_build:
	gcc -o usr/bin/getepoch src/getepoch.c -lcurl

override_dh_installsystemd:
	dh_installsystemd -padaptation-exynos9810-configs --name=apt-fix apt-fix.service --no-start
	dh_installsystemd -padaptation-exynos9810-configs --name=altresolv altresolv.service --no-start
	dh_installsystemd -padaptation-exynos9810-configs --name=bluetoothalt bluetoothalt.service --no-start
	dh_installsystemd -padaptation-exynos9810-configs --name=audio-hidl audio-hidl.service --no-start
	dh_installsystemd -padaptation-exynos9810-configs --name=getepoch getepoch.service --no-start
	dh_installsystemd -padaptation-exynos9810-configs --name=hadess-sensorfw-proxy hadess-sensorfw-proxy.service --no-start
	dh_installsystemd -padaptation-exynos9810-configs --name=sensorfw-exynos-config sensorfw-exynos-config.service --no-start
	dh_installsystemd -padaptation-exynos9810-configs --name=enable-nfc enable-nfc.service --no-start
